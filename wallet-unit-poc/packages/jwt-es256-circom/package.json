{
  "name": "@privacy-scaling-explorations/jwt-es256-circom",
  "description": "circom circuits for verifying JWTs signed with ES256",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/privacy-scaling-explorations/zkID"
  },
  "scripts": {
    "start": "npx ts-node ./src/index.ts",
    "test": "NODE_OPTIONS=--max-old-space-size=8192 npx mocha --timeout 900000",
    "save_input_file": "npx ts-node src/save_inputs.ts",
    "compile": "npx circomkit compile add && bash script.sh",
    "setup": "npx circomkit setup add",
    "prove": "npx circomkit prove add",
    "verify": "npx circomkit verify add"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@noble/curves": "^1.8.1",
    "@noble/hashes": "^1.7.1",
    "@types/asn1": "^0.2.4",
    "@zk-email/circuits": "^6.3.3-nightly-20250214",
    "@zk-email/helpers": "^6.4.2",
    "asn1": "^0.2.6",
    "base64-js": "^1.5.1",
    "bs58": "^6.0.0",
    "circom_tester": "^0.0.21",
    "circomkit": "^0.3.3",
    "circomlib": "^2.0.5",
    "did-resolver": "^4.1.0",
    "jose": "^6.0.10",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "jws": "^4.0.0",
    "key-did-resolver": "^4.0.0",
    "mocha": "^11.1.0",
    "safe-buffer": "^5.2.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.0",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@4.7.0+sha512.5a0afa1d4c1d844b3447ee3319633797bcd6385d9a44be07993ae52ff4facabccafb4af5dcd1c2f9a94ac113e5e9ff56f6130431905884414229e284e37bb7c9"
}
